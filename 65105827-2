#Algorithm check Odd or Event
// Step 1: Initialize Variables
double number = 15.0;
  
// Step 2: Check Even or Odd
main(){
  if (number % 2 == 0){ //เปรียบเทียบใช้ ==
    print("$number is even.");
  }
  else {
    print("$number is odd.");
  }
  
// Step 3: Print Numbers from 1 to 5
  for (int i = 1; i <= 5; i++) { //ต้องกำหนดประเภท i
    print("Number: $i");
  }
}
